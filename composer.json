{
  "name": "awwar/php-http-entity-manager",
  "description": "Doctrine-like http client",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Awwar\\PhpHttpEntityManager\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Awwar\\PhpHttpEntityManager\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "*",
    "squizlabs/php_codesniffer": "*",
    "phpunit/phpunit": ">=8 <10"
  }
}
